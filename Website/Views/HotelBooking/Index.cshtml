@{
}

@model List<HotelBookingDay66Api.Dto.HotelBookingDto>
<div class="row">
    <div class="col-md-12"><h5 class="bg-black text-warning p-3">API Test - All Booking Hotel Listing</h5></div>
    <div class="col-md-12">
        <table class="table table-bordered">
            <tr class="bg-warning">
         <th>HotelId</th>
            <th>HotelName</th>
            <th>HotelLocation</th>
            <th>RoomRefId</th>
            <th>HotelRating</th>
    </tr>
    @foreach(var bookHotel in Model)
    {
        <tr>
            <td>@bookHotel.HotelId</td>
            <td>@bookHotel.HotelName</td>
            <td>@bookHotel.HotelLocation</td>
            <td>@bookHotel.RoomRefId</td>
            <td>@bookHotel.HotelRating</td>
        </tr>
      }
        </table>
    </div>
</div>